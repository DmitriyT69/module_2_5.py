def get_matrix(n, m, value):
    matrix = []
    for i in range(n):
        matrix.append(n)
        for j in range(m):
            matrix.append(value)
    print(matrix)
    return matrix

result_1 = get_matrix(2,2,10)
result_2 = get_matrix(3, 4 , 38)
result_3 = get_matrix(4, 2, 11)
print(result_1)
print(result_2)
print(result_3)
